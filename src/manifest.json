{
  "package": "com.gaozhongkui.test",
  "name": "万能下载器",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1070,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.file"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    },
    {
      "origin": "system.storage"
    },
    {
      "origin": "system.file"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "pages/Main",
    "pages": {
      "pages/Home": {
        "component": "index"
      },
      "pages/Main": {
        "component": "index"
      },
      "pages/Settings": {
        "component": "index"
      },
      "pages/Profile": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "pages": {
      "pages/Home": {
        "titleBarText": "万能下载器"
      },
      "pages/Main": {
        "titleBarText": "万能下载器"
      },
      "pages/Settings": {
        "titleBarText": "设置页面"
      },
      "pages/Profile": {
        "titleBarText": "网页"
      }
    }
  }
}